<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace=绑定一个对应的Dao/Mapper接口-->
<mapper namespace="com.example.cse.Mapper.UserMapper">
    <!--select查询语句-->
    <select id="findAll" resultType="com.example.cse.Entity.UserClass.User">
        select * from `cse`.`user`;
    </select>

    <select id="getUserByNamePass" parameterType="String" resultType="com.example.cse.Entity.UserClass.User">
        select * from  `cse`.`user` where `cse`.`user`.`UserCode` = #{name} and `UserPass` = #{pass} ;
    </select>
</mapper>
